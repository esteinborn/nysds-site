{% extends "layouts/base.njk" %}

{% block content %}
<div class="nys-grid-container-widescreen">
<div class="nys-grid-row nys-grid-gap">
  <aside class="nys-desktop:nys-grid-col-2" data-pagefind-ignore>
    {% include "partials/section-nav.njk" %}
  </aside>
  <div class="nys-desktop:nys-grid-col-8">
    {{ content | safe }}
  </div>
  <aside class="nys-desktop:nys-grid-col-2" data-pagefind-ignore>
    {% include "partials/onpage-nav.njk" %}
  </aside>
</div>
</div>
{% endblock %}